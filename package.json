{
  "name": "drf-website",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "husky": "^1.3.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "lint-staged": "^8.1.0",
    "lodash.flow": "^3.5.0",
    "next": "^7.0.2",
    "next-images": "^1.0.4",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-lazy-images": "^1.1.0",
    "react-typeform-embed": "^0.2.0",
    "smoothscroll-polyfill": "^0.4.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "prettier": "^1.15.3",
    "scssfmt": "^1.0.7"
  },
  "engines": {
    "node": "^11.0.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "prettier": "prettier --write **/*.js **/*.jsx",
    "heroku-postbuild": "next build"
  },
